{
  "name": "dooray-mcp-server",
  "version": "1.0.0",
  "description": "Dooray API MCP Server for interacting with Dooray services",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "dooray-mcp": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mcp",
    "dooray",
    "api",
    "model-context-protocol",
    "nhn"
  ],
  "author": "NHN",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/dooray-mcp-server.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "axios": "^1.6.2",
    "dotenv": "^17.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
